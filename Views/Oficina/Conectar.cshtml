@model IEnumerable<TotalClean.Models.Usuario>
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <!-- Title Page -->  
    <title>Login</title>
 
    <!-- CSS -->
    <link rel="stylesheet" href="~/css/estilo.css" />

    
</head>
<body>
    <div id="container">
        <form asp-controller="Oficina" asp-action="Conectado">
            <!-- Username -->
            <label for="name">Username:</label>
            <input type="name">
            <!-- Password -->
            <label for="username">Password:</label>
            
            <input type="password" name="usuario">
            <div id="lower">
                
                <!-- Submit Button -->
                <input type="submit" value="Conectar">
            </div>
        </form>       
    </div>
</body>

</html>
<a class="btn btn-primary" href="Inicio">Volver</a> 